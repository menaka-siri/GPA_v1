<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="GPABundle\Entity\Users" table="users">
    <id name="id" type="string" column="id" length="7">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="password" type="string" column="password" length="50" nullable="false"/>
    <field name="firstName" type="string" column="first_name" length="20" nullable="false"/>
    <field name="lastName" type="string" column="last_name" length="20" nullable="false"/>
    <field name="email" type="string" column="email" length="30" nullable="false"/>
    <field name="mobile" type="integer" column="mobile" nullable="true"/>
    <field name="department" type="string" column="department" length="10" nullable="false"/>
    <field name="profession" type="integer" column="profession" nullable="false"/>
    <field name="status" type="integer" column="status" nullable="false"/>
  </entity>
</doctrine-mapping>
